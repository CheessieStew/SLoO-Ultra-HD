<UserControl x:Class="sloo.EditorControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:sloo"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid>
        <ListBox x:Name="listBox" HorizontalAlignment="Left" Height="100" Margin="137,146,0,0" VerticalAlignment="Top" Width="100"/>
        <StackPanel>
        <Slider Value="{Binding Volume }" />
        <Slider Value="{Binding Frequency}" />
        </StackPanel>
    </Grid>
</UserControl>
